{
  "name": "custom-react",
  "version": "1.6.1",
  "description": "some custom react stuff",
  "main": "dist/umd/index.js",
  "module": "dist/es/index.js",
  "sideEffects": false,
  "scripts": {
    "start": "webpack-dev-server",
    "prebuild": "rimraf dist",
    "build:webpack": "webpack --env.prod",
    "pretranspile": "rimraf lib/",
    "build": "rollup -c",
    "lint": "eslint --ext js,jsx src",
    "prsetest": "npm run lint",
    "test": "jest src",
    "test:watch": "jest src --watch",
    "test:coverage": "jest src --coverage",
    "test:coveralls": "jest src --coverage --coverageReporters=text-lcov | coveralls",
    "test:e2e": "jest e2e/",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepublishOnly": "npm run build",
    "pap": "jest tester.test.js "
  },
  "filess": [
    "dist"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm run test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yurikrupnik/custom-react.git"
  },
  "author": {
    "name": "Yuri Krupnik",
    "email": "krupnik.yuri@gmail.com"
  },
  "license": "ISC",
